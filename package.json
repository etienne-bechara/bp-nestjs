{
  "name": "nestjs-boilerplate",
  "version": "0.1.0",
  "description": "Opinated Serverless Boilertplate Based on NestJS and Written in TypeScript",
  "author": {
    "name": "Etienne Bechara",
    "email": "et.rodrigues@hotmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/etienne-bechara/nestjs-boilerplate"
  },
  "license": "MIT",
  "scripts": {
    "start": "nest start",
    "dev": "nest start --debug --watch",
    "lint": "eslint **/*.ts",
    "build": "nest build",
    "blueprint:crud": "ts-node ./scripts/blueprint/blueprint.generate -t crud",
    "update:patch": "ncu -u --semverLevel minor && npm i && npm audit fix",
    "update:minor": "ncu -u --semverLevel major && npm i && npm audit fix",
    "update:major": "ncu -u && npm i && npm audit fix --force",
    "update:boilerplate": "git fetch boilerplate && git merge boilerplate/master --allow-unrelated-histories",
    "cache:reset": "mikro-orm cache:clear && nest build && mikro-orm cache:generate",
    "schema:preview": "npm run cache:reset && mikro-orm schema:update --dump",
    "schema:update": "npm run cache:reset && mikro-orm schema:update --run"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./dist/core/orm/orm.settings.js"
    ]
  },
  "dependencies": {
    "@nestjs/common": "7.3.1",
    "@nestjs/core": "7.3.1",
    "@nestjs/platform-express": "7.3.1",
    "@sentry/node": "5.19.0",
    "axios": "0.19.2",
    "chalk": "4.1.0",
    "class-transformer": "0.2.3",
    "class-validator": "0.12.2",
    "dotenv": "8.2.0",
    "flat": "5.0.0",
    "globby": "11.0.1",
    "mikro-orm": "3.6.15",
    "moment": "2.27.0",
    "mysql2": "2.1.0",
    "nestjs-mikro-orm": "1.0.3",
    "qs": "6.9.4",
    "redis": "3.0.2",
    "reflect-metadata": "0.1.13",
    "request-ip": "2.1.3",
    "rxjs": "6.6.0",
    "user-agents": "1.0.559",
    "uuid": "8.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "7.4.1",
    "@nestjs/schematics": "7.0.1",
    "@types/fs-extra": "9.0.1",
    "@types/node": "14.0.14",
    "@types/redis": "2.8.23",
    "@types/user-agents": "1.0.0",
    "@typescript-eslint/eslint-plugin": "3.5.0",
    "@typescript-eslint/parser": "3.5.0",
    "change-case": "4.1.1",
    "eslint": "7.3.1",
    "eslint-plugin-more": "1.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "fs-extra": "9.0.1",
    "npm-check-updates": "7.0.1",
    "ts-node": "8.10.2",
    "typescript": "3.9.6",
    "yargs": "15.4.0"
  }
}
