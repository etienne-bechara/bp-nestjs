{
  "name": "nestjs-boilerplate",
  "version": "0.1.0",
  "description": "Opinated Serverless Boilertplate Based on NestJS and Written in TypeScript",
  "author": {
    "name": "Etienne Bechara",
    "email": "et.rodrigues@hotmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/etienne-bechara/nestjs-boilerplate"
  },
  "license": "MIT",
  "scripts": {
    "start": "nest start",
    "dev": "nest start --debug --watch",
    "lint": "eslint **/*.ts",
    "build": "nest build",
    "update:minor": "npm update && npm audit fix",
    "update:major": "ncu -u && npm i && npm audit fix --force",
    "update:boilerplate": "git fetch boilerplate && git merge boilerplate/master --allow-unrelated-histories"
  },
  "dependencies": {
    "@nestjs/common": "^7.2.0",
    "@nestjs/core": "^7.2.0",
    "@nestjs/platform-fastify": "^7.2.0",
    "@nestjs/typeorm": "^7.1.0",
    "@sentry/node": "^5.18.0",
    "axios": "^0.19.2",
    "chalk": "^4.1.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "fastify": "^2.15.0",
    "moment": "^2.27.0",
    "mysql": "^2.18.1",
    "qs": "^6.9.4",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^2.1.3",
    "rxjs": "^6.5.5",
    "typeorm": "^0.2.25",
    "user-agents": "^1.0.559"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.4.1",
    "@nestjs/schematics": "^7.0.1",
    "@types/node": "^14.0.13",
    "@types/redis": "^2.8.22",
    "@types/user-agents": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "eslint": "^7.3.1",
    "eslint-plugin-more": "^1.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
}
